<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:chrome="http://www.mozilla.org/rdf/chrome#">

  <!-- List all the skins being supplied by this theme -->
  <Seq about="urn:mozilla:skin:root">
    <li resource="urn:mozilla:skin:pixelzilla"/>
  </Seq>

  <!-- classic Information -->
  <Description about="urn:mozilla:skin:pixelzilla"
    chrome:displayName="PixelZilla"
    chrome:author="Roman Joost"
    chrome:authorURL="http://www.romanofski.de"
    chrome:accessKey="P"
    chrome:description="A theme based on pixel art"
    chrome:name="pixelzilla"
    chrome:image="preview.png">

    <chrome:packages>
      <Seq about="urn:mozilla:skin:pixelzilla:packages">
        <li resource="urn:mozilla:skin:pixelzilla:global"/>
        <li resource="urn:mozilla:skin:pixelzilla:browser"/>
        <li resource="urn:mozilla:skin:pixelzilla:mozapps"/>
				<li resource="urn:mozilla:skin:pixelzilla:help"/>
      </Seq>
    </chrome:packages>
  </Description>

  <!-- Version Information.  State that we work only with major version 1 of this package. -->
  <Description chrome:skinVersion="1.5" about="urn:mozilla:skin:pixelzilla:global"/>
  <Description chrome:skinVersion="1.5" about="urn:mozilla:skin:pixelzilla:browser"/>
  <Description chrome:skinVersion="1.5" about="urn:mozilla:skin:pixelzilla:mozapps"/>
	<Description chrome:skinVersion="1.5" about="urn:mozilla:skin:pixelzilla:help"/>

</RDF>

